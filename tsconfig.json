{
  "compilerOptions": {
    "allowJs": false,
    "target": "esnext",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["esnext", "es2017.object", "dom.iterable", "webworker"],
    "module": "esnext"
  },
  "include": [
    "*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
